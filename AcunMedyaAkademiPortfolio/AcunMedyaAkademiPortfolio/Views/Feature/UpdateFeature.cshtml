@model AcunMedyaAkademiPortfolio.Models.TblFeature
@{
    ViewBag.Title = "UpdateFeature";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>UpdateFeature</h2>
<form method="post">
    <label>Hakkında id</label>
    @Html.TextBoxFor(x => x.FeatureID, new { @class = "table table-bordered" })
    <br />

    <label>Başlık</label>
    @Html.TextBoxFor(x => x.NameSurname, new { @class = "table table-bordered" })
    <br />

    <label>Açıklama</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "table table-bordered" })
    <br />

    <label>Proje Adı</label>
    @Html.TextBoxFor(x => x.ProjectName, new { @class = "table table-bordered" })
    <br />

    <label>Resim</label>
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "table table-bordered" })
    <br />

    <button class="btn btn-success">Hizmetlerim Güncelle</button>
</form>



