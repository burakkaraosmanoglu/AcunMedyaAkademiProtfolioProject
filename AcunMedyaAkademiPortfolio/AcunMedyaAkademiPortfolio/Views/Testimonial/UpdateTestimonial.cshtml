@model AcunMedyaAkademiPortfolio.Models.TblTestimonial
@{
    ViewBag.Title = "UpdateTestimonial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>UpdateTestimonial</h2>
<form method="post">
    <label>proje id</label>
    @Html.TextBoxFor(x => x.TestimonialID, new { @class = "table table-bordered" })
    <br />

    <label>Başlık</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "table table-bordered" })
    <br />

    <label>Açıklama</label>
    @Html.TextBoxFor(x => x.Description, new { @class = "table table-bordered" })
    <br />

    <label>Ad Soyad</label>
    @Html.TextBoxFor(x => x.NameSurname, new { @class = "table table-bordered" })
    <br />

    <label>Resim</label>
    @Html.TextBoxFor(x => x.ImageUrl, new { @class = "table table-bordered" })
    <br />

    <label>İş</label>
    @Html.TextBoxFor(x => x.Job, new { @class = "table table-bordered" })
    <br />

    <label>Statü</label>
    @Html.TextBoxFor(x => x.Status, new { @class = "table table-bordered" })
    <br />

    <button class="btn btn-success">proje güncelle</button>
</form>


